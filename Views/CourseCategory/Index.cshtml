@model IEnumerable<KidsCenter.Models.CourseCategoryModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CourseCategoryId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CourseCategoryName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CourseCategoryId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CourseCategoryName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CourseCategoryId }) |
            @Html.ActionLink("Details", "Details", new { id = item.CourseCategoryId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.CourseCategoryId })
        </td>
    </tr>
}

</table>
