@model IEnumerable<KidsCenter.Models.AgeCategoryModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AgeCategoryId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AgeCategoryName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AgeCategoryId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AgeCategoryName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AgeCategoryId }) |
            @Html.ActionLink("Details", "Details", new { id = item.AgeCategoryId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.AgeCategoryId })
        </td>
    </tr>
}

</table>
